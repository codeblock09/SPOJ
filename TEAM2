#include<iostream>
#include<algorithm>
#define ll long long int
using namespace std;
int main()
{
    ll a=0,b=0,c=0,d=0,ct=1;
    while(cin>>a>>b>>c>>d)
    {
        ll p[4]={a,b,c,d};
        sort(p,p+4);
        ll ans=p[3]+p[2];
        cout<<"Case "<<ct<<": "<<ans<<endl;
        ct++;
    }
    return 0;
} 
